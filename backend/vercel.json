{
  "version": 2,
  "buildCommand": "npm run build",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/api/index.ts"
    }
  ]
}