{
  "version": 2,
  "buildCommand": "npm run build",
  "env": {
    "DATABASE_URL": "@database_url",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 300,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/api/index.js"
    }
  ]
}